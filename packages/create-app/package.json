{
  "name": "@youcan/create-app",
  "type": "module",
  "version": "1.1.0-beta.10",
  "description": "A CLI for creating YouCan Shop apps",
  "author": "YouCan <contact@youcan.shop> (https://youcan.shop)",
  "license": "MIT",
  "keywords": [
    "youcan",
    "youcan-cli",
    "youcan-app"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "bin": {
    "create-app": "./bin/exec.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc --noEmit && rollup --config rollup.config.js",
    "dev": "shx rm -rf dist && rollup --config rollup.config.js --watch",
    "release": "pnpm publish --access public",
    "type-check": "tsc --noEmit",
    "relink": "pnpm link --global"
  },
  "dependencies": {
    "@oclif/core": "^2.15.0",
    "@youcan/cli-kit": "workspace:*"
  },
  "devDependencies": {
    "@oclif/plugin-legacy": "^1.3.0",
    "@types/node": "^18.18.0",
    "shx": "^0.3.4"
  },
  "oclif": {
    "bin": "create-app",
    "dirname": "create-app",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {}
  }
}
