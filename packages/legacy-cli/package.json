{
  "name": "@youcan/legacy-cli",
  "type": "module",
  "version": "1.1.0-beta.3",
  "description": "YouCan legacy themes CLI.",
  "author": "YouCan <contact@youcan.shop> (https://youcan.shop)",
  "keywords": [],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/dts/index.d.ts",
  "bin": {
    "youcan-legacy": "bin/index.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc --noEmit && rollup --config rollup.config.js",
    "dev": "shx rm -rf dist && rollup --config rollup.config.js --watch",
    "release": "pnpm publish --access public",
    "type-check": "tsc --noEmit",
    "relink": "pnpm link --global"
  },
  "dependencies": {
    "archiver": "^5.3.2",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "decompress": "^4.2.1",
    "execa": "^6.1.0",
    "formdata-node": "^5.0.1",
    "kleur": "^4.1.5",
    "node-fetch": "^3.3.2",
    "prompts": "^2.4.2",
    "puppeteer": "^19.11.1",
    "ramda": "^0.28.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@azuradara/eslint-config-base": "^0.1.3",
    "@azuradara/eslint-config-ts": "^0.1.3",
    "@oclif/plugin-autocomplete": "^2.3.8",
    "@oclif/plugin-commands": "^2.2.26",
    "@types/archiver": "^5.3.3",
    "@types/file-saver": "^2.0.5",
    "@types/node": "^18.18.0",
    "@types/prompts": "^2.4.5",
    "@types/ramda": "^0.28.25",
    "@types/rimraf": "^3.0.2",
    "esno": "^0.16.3",
    "pnpm": "^7.33.6",
    "rimraf": "^3.0.2",
    "rollup": "^3.29.3",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-node-externals": "^6.1.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "shx": "^0.3.4",
    "typescript": "^4.9.5"
  }
}
