    "change-case": "^4.1.2",
    "env-paths": "^3.0.0",
    "execa": "^6.1.0",
    "fs-extra": "^11.1.1",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2",
    "simple-git": "^3.20.0",
    "tempy": "^3.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^18.18.0",
    "@types/prompts": "^2.4.5",
    "shx": "^0.3.4"
  }
}
