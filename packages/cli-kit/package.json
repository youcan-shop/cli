{
  "name": "@youcan/cli-kit",
  "type": "module",
  "version": "1.0.0",
  "description": "Utilities for the YouCan CLI",
  "author": "YouCan <contact@youcan.shop> (https://youcan.shop)",
  "license": "MIT",
  "keywords": [
    "youcan",
    "youcan-cli",
    "youcan-app"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc --noEmit && rollup --config rollup.config.js",
